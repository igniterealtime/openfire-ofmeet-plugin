<plugin>
    <class>org.jivesoftware.openfire.plugin.ofswitch.OfSwitchPlugin</class>
    <name>${project.name}</name>
    <description>${project.description}</description>
    <author>Ignite Realtime</author>
    <version>${project.version}</version>
    <date>06/30/2017</date>
    <minServerVersion>4.0.0</minServerVersion>
    <adminconsole>
        <tab id="tab-ofswitch" name="${plugin.title}" url="freeswitch-settings.jsp" description="${plugin.title.description}">
            <sidebar id="siderbar-freeswitch"  name="${plugin.title}">
                <item id="freeswitch-overview"  	name="${config.freeswitch.overview.name}"  	description="${config.freeswitch.overview.description}"  	url="freeswitch-overview.jsp"/>
                <item id="freeswitch-settings"  	name="${config.freeswitch.settings.name}"  	description="${config.freeswitch.settings.description}"  	url="freeswitch-settings.jsp"/>
                <item id="freeswitch-users"  		name="${config.freeswitch.users.name}"  	description="${config.freeswitch.users.description}"  		url="freeswitch-users.jsp"/>
                <item id="freeswitch-conferences"  	name="${config.freeswitch.conferences.name}"  	description="${config.freeswitch.conferences.description}"  	url="freeswitch-conferences.jsp"/>
                <item id="freeswitch-calls"  		name="${config.freeswitch.calls.name}"  	description="${config.freeswitch.calls.description}"  		url="freeswitch-calls.jsp"/>
                <item id="freeswitch-registrations"  	name="${config.freeswitch.registrations.name}"  description="${config.freeswitch.registrations.description}"  	url="freeswitch-registrations.jsp"/>                
                <item id="freeswitch-gateways"  	name="${config.freeswitch.gateways.name}"  	description="${config.freeswitch.gateways.description}"  	url="freeswitch-gateways.jsp"/>                
                <item id="freeswitch-agents"  		name="${config.freeswitch.agents.name}"  	description="${config.freeswitch.agents.description}"  		url="freeswitch-agents.jsp"/>                
                <item id="freeswitch-queues"  		name="${config.freeswitch.queues.name}"  	description="${config.freeswitch.queues.description}"  		url="freeswitch-queues.jsp"/>                
                <item id="freeswitch-tiers"  		name="${config.freeswitch.tiers.name}"  	description="${config.freeswitch.tiers.description}"  		url="freeswitch-tiers.jsp"/>                
                <item id="freeswitch-verto"  		name="${config.freeswitch.verto.name}"  	description="${config.freeswitch.verto.description}"  		url="freeswitch-verto.jsp"/>                
             </sidebar>                           
        </tab>
    </adminconsole>    
</plugin>
